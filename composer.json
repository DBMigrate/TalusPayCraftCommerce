{
    "name": "db-migrate/commerce-taluspay",
    "description": "Talus Pay plugin for craft commerce",
    "type": "craft-plugin",
    "version": "v1.0.12",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "commerce-taluspay"
    ],
    "support": {
        "issues": "https://github.com/DBMigrate/TalusPayCraftCommerce/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "DBMigrate",
            "homepage": "http://github.com/dbmigrate"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "omnipay/common": "^3",
        "php-http/guzzle6-adapter": "*",
        "craftcms/commerce": "^3.0",
        "craftcms/commerce-omnipay": "^2.0",
        "db-migrate/omnipay-taluspay": "^2.0"
    },
    "autoload": {
        "psr-4": {
          "DBMigrate\\commercetaluspay\\": "src/"
        }
    },
    "extra": {
        "name": "Talus Pay for Craft Commerce",
        "handle": "commerce-taluspay",
        "hasCpSettings": false,
        "hasCpSection": false,
        "basePath": "",
        "changelogUrl": "https://raw.githubusercontent.com/DBMigrate/TalusPayCraftCommerce/master/CHANGELOG.md",
        "components": {
        },
        "class": "DBMigrate\\commercetaluspay\\CommerceTaluspay"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
